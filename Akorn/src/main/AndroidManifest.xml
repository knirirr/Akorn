<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.akorn.akorn"
    android:installLocation="auto"
    android:versionCode="15"
    android:versionName="0.2.0-alpha" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.akorn.akorn.ViewingActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.akorn.akorn.SettingsActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="org.akorn.akorn.FilterActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="org.akorn.akorn.CreateAccountActivity"
            android:label="Akorn">
        </activity>
        <provider
            android:name=".contentprovider.AkornContentProvider"
            android:authorities="org.akorn.akorn.contentprovider"
            android:exported="false">
        </provider>
        <service
            android:name="AkornSyncService"
            android:icon="@drawable/ic_launcher"
            android:label="@string/service_name">
        </service>
     </application>


</manifest>
